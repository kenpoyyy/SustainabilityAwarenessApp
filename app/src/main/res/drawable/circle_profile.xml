<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="rectangle">
            <solid android:color="#AAC477" /> <!-- Green outline -->
            <corners android:radius="360dp" />
        </shape>
    </item>
    <item android:right="3dp" android:left="3dp" android:top="3dp" android:bottom="3dp">
        <shape android:shape="rectangle">
            <solid android:color="#FFFFFF" /> <!-- White filling -->
            <corners android:radius="360dp" /> <!-- Adjust the corner radius if needed -->
        </shape>
    </item>
</layer-list>